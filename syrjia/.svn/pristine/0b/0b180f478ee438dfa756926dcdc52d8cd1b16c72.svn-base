	/**
	 * 查询医生单元
	 * @param sql
	 * @param column
	 * @return
	 */
	private Object queryDoctorsUnitCol(String sql, String column){
		List<Map<String, Object>> list = this.jdbcTemplate.queryForList(sql);
		if(null != list && list.size()>0){
