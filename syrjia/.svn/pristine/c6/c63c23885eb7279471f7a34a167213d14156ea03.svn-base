.header{background: #fff;width: 100%; position: relative;border-bottom: 1px solid #d1d1d1;}
.header_back{display: block;height:0.44rem;width: 0.1rem;background: url("../img/back.png")no-repeat left center;background-size: 0.1rem auto;padding-right: 0.5rem;position:absolute;left: 0.1rem;top:0;}
.header_right{line-height: 0.44rem;font-size: 0.14rem;position: absolute;right: 0.1rem;top:0;display: block;padding-left: 0.5rem;}
.header h3{font-size: 0.16rem;text-align: center;line-height: 0.44rem;font-weight: normal;}
.book_site{background: #fff;padding: 0 0.0625rem;overflow-x: auto;display: -webkit-box;-webkit-box-orient: horizontal;height: 0.25rem;}
.book_site li{width: 15%;text-align: center;font-size: 0.12rem;line-height: 0.25rem;color: #666;}
.book_site a{font-size: 0.08rem;line-height: 0.15rem;vertical-align: top;color: #999;display:block;width: 12%;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.book_site a span{display: block;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.book_site .book_btnli{color:#ff832d;}
.book_site .book_width{width: 20%;}
.book_site:after{content: "";display: block;clear: both;}

.book_explain{display: -webkit-box;-webkit-box-orient: horizontal;-webkit-box-pack: center;position:absolute;bottom:0.9rem;left: 0;width:100%;}
.book_explain li{line-height: 0.15rem;height: 0.15rem;color:#999;font-size: 0.12rem;margin-right: 0.1rem;}
.book_explain li:last-child{margin-right: 0;}
.book_explain li span{display:inline-block;margin-right: 0.05rem;vertical-align: bottom;}
.book_explain li span:first-child{width: 0.18rem;height: 0.15rem;}
.book_explain li:first-child span:first-child{background: #b7bdbf;}
.book_explain li:nth-of-type(2) span:first-child{background: #2bb061;}
.book_explain li:nth-of-type(3) span:first-child{background: #ff832d url("../img/book_check.png")no-repeat center;background-size: 0.1rem auto;}
.book_explain li:nth-of-type(4) span:first-child{background: #f9c325 url("../img/book_check.png")no-repeat center;background-size: 0.1rem auto;}
.book_explain li:nth-of-type(5) span:first-child{background: #29c2f9 url("../img/lock.png")no-repeat center;background-size: 0.1rem auto;}


.book_bot{background: #fff;position:fixed;bottom:0;left: 0;width: 100%;}
.book_bot p {
    line-height: 0.3rem;
    text-align: center;
    font-size: 0.12rem;
    color: #999;
}
.book-orderinfo{display: -webkit-box;-webkit-box-orient: horizontal;-webkit-box-pack: center;overflow-x: auto;overflow-y: hidden;}
.book-orderinfo li{line-height: 0.25rem;height: 0.25rem;text-align: center;border:1px solid #ff832d;border-radius: 6px;margin:0.05rem 0.05rem;}
.book-orderinfo li:last-child{margin-right: 0;}
.book-orderinfo li span{font-size: 0.12rem;padding: 0 0.1rem;display: inline-block;vertical-align: bottom;color: #ff832d;}
/*.book-orderinfo li a{display: inline-block;float: right;width: 0.1rem;height: 0.3rem;background: url("../img/delete.png")no-repeat left center;background-size: 0.1rem auto;padding-right: 0.1rem;}*/
.book_submit{margin: 0 0.25rem 0.08rem;text-align: center;display: block;line-height: 0.4rem;color: #fff;font-size: 0.16rem;background: #2bb061;border-radius: 6px;}

.book-date {
    border-bottom: 1px solid #e1e1e1;
    background: #fff;
}
/*.book-date .data-change, .book-date li {*/
    /*height: 0.4rem;*/
/*}*/
.book-date .data-change {
    float: right;
    width: 0.55rem;
    margin-left: 0;
    position: relative;
}
.book-date .data-change img {
    width: 0.12rem;
    position: absolute;
    left: 0.215rem;
    top:0.1rem;
}
.book-date .date-wrap {
    overflow-x: auto;
}
.book-date .date-wrap ul {
    position: relative;
    width: 152%;
}
.book-date .date-wrap ul:after {
    content: "\0020";
    display: block;
    height: 0;
    clear: both;
    overflow: hidden;
    visibility: hidden;
}
.book-date .date-wrap ul .active {
    color: #ff832d;
    border-bottom: 1px solid #ff832d;
}
.book-date .date-wrap ul li {
    color: #666;
    float: left;
    width: 0.8rem;
    border-right: 1px solid #e1e1e1;
    /*background: url(../img/border-r.jpg) right top repeat-y;*/
    padding: 0.04rem 0;
}
.book-date .date-wrap ul a:last-child li{border: none;}
.book-date .date-wrap ul li p:nth-of-type(1) {
    font-size: 0.12rem;
}
.book-date .date-wrap ul li p:nth-of-type(2) {
    font-size: 0.12rem;
   /*  color: #999; */
}
.book-date .date-wrap ul li p:nth-of-type(3) {
    font-size: 0.12rem;
}

.book-date .expend {
    transform: rotate(180deg);
    -webkit-transform: rotate(180deg);
}
/*.book-date .bg {*/
    /*background: url(../img/border-r.jpg) right top repeat-y, url(../img/border-r.jpg) left top repeat-y;*/
/*}*/
.book-date .expend + .date-wrap ul {
    left: -2.4rem;
}
/*loding加载*/
.loding{position: fixed;left: 0;top: 50%;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);width: 100%;z-index: 999;height:1rem!important;}
.loding_img{display: table;width: 1.1rem;overflow: hidden;margin: 0 auto;}
.loding_img span{display: table-cell;vertical-align: middle;text-align: center;}
.loding_img span img{max-width: 1.1rem;height: auto;vertical-align: top;border-radius: 8px;}
.loding p{font-size: 0.14rem;color: #fff;line-height: 0.35rem;text-align: center;position: absolute;bottom:0;left:0;width: 100%;}
/*loding加载   end*/

/*我的预定，弹框——开始*/
.book_layer{position: fixed;width: 100%;height: 100%;background: rgba(0,0,0,0.4);top:0;left: 0;z-index: 199;}
.book_been{background: #fff;width: 80%;position: fixed;top:50%;left:10%;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);z-index: 200;border-radius: 4px;}
.book_been h3{line-height: 0.44rem;font-size: 0.15rem;text-align: center;font-weight: normal;border-bottom: 1px solid #ddd;}
.book_been_con{padding: 0.1rem 0.2rem;border-bottom: 1px solid #ddd;}
.book_been_con li{text-align: left;display: -webkit-box;}
.book_been_con li span{font-size: 0.14rem;line-height: 0.3rem;display: inherit;}
.book_been_con li span:first-child{color: #666;}
.book_been_con li span a{margin-right: 0.1rem;}
.book_been_btn{display: -webkit-box;-webkit-box-pack: center;}
.book_been_btn a{display: block;width: 50%;text-align: center;font-size: 0.15rem;line-height:0.44rem;height:0.44rem;position: relative;}
.book_been_btn a:first-child:after{content: "";display: inline-block;width: 1px;height: 100%;background: #ddd;position: absolute;right: 0;top:0;}
.book_been_btn a:last-child{color: #ff662a;}
/*我的预定，弹框——结束*/
.book_refund{position: fixed;width: 100%;height: 100%;background: rgba(0,0,0,0.4);top:0;left: 0;z-index: 199;}
.book_way{max-width: 192px;}

@media(max-width:320px){
    .book_way{max-width: 174px;}
}
@media(min-width:414px){
    .book_way{max-width: 212px;}
}
/*选择场地，新增弹窗 start*/
.book_hint{width: 68%;position: fixed;left: 16%;top: 50%;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);border-radius: 12px;overflow: hidden;z-index:199;}
.book_hint_top{padding: 0.1rem 0 0.2rem;margin-bottom: 1px;background: rgba(255,255,255,0.8);}
.book_hint_top h3{font-size: 0.15rem;line-height: 0.3rem;}
.book_hint_top p{font-size: 0.12rem;line-height:0.18rem;text-align: center;padding: 0 0.18rem;}
.book_hint_btn{display: -webkit-box;-webkit-box-pack: center;}
.book_hint_btn a{display: block;width: 50%;font-size:0.15rem;position: relative;line-height: 0.44rem;background: rgba(255,255,255,0.8);color: #1184fc;}
.book_hint_btn a:first-child{margin-right: 1px;}
/*选择场地，新增弹窗 end*/